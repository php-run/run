{
  "name": "baselrabia/php_playground",
  "description": "This package holds the content of play ground for php including source code, references and more",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Basel Rabia",
      "email": "baselrabia@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Run\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "src/Steps/Helper/helpers.php"
    ]
  },
  "require": {
    "symfony/console": "^5.4",
    "symfony/var-dumper": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
